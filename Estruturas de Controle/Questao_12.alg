algoritmo "Questão_12"
var
num1, num2: inteiro
inicio
Escreval("Digite o primeiro número: ")
leia(num1)
Escreval("Digite o segundo número: ")
leia(num2)

se num1 > num2 entao
   Escreval(" ")
   Escreval("Numeros em ordem crescente:",num2, " e",num1)
senao
   Escreval(" ")
   Escreval("Numeros em ordem crescente:",num1, " e",num2)
fimse
fimalgoritmo
